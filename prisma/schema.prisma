generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id          Int      @id @default(autoincrement())
  title       String   @db.VarChar(255)
  description String?
  imgUrl      String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}